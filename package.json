{
  "name": "topi-channel",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/electron/static.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --host 0.0.0.0 --disable-host-check true\" \"electron src/electron/live.js --disable-gpu\"",
    "test": "ng lint && ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "package:linux": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=linux --arch=all --overwrite",
    "package:mac": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=darwin --arch=all --overwrite ",
    "package:win": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=win32 --arch=all --overwrite ",
    "package:all": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --all --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/router": "4.3.1",
    "angular2-moment": "1.6.0",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "ng2-youtube-player": "0.0.3",
    "rxjs": "5.4.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.3",
    "@angular/compiler-cli": "4.3.1",
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.14",
    "@types/youtube": "0.0.30",
    "codelyzer": "3.1.2",
    "concurrently": "3.5.0",
    "devtron": "1.4.0",
    "electron": "1.6.11",
    "electron-debug": "1.2.0",
    "electron-packager": "8.7.2",
    "electron-prebuilt": "1.4.13",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-electron": "5.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.2",
    "ts-node": "3.2.1",
    "tslint": "5.5.0",
    "typescript": "2.4.2"
  }
}
