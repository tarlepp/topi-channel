{
  "name": "topi-channel",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/electron/static.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --host 0.0.0.0\" \"electron src/electron/live.js --disable-gpu\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "package:linux": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=linux --arch=all --overwrite",
    "package:mac": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=darwin --arch=all --overwrite ",
    "package:win": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --platform=win32 --arch=all --overwrite ",
    "package:all": "ng build --prod && electron-packager . $npm_package_name-$npm_package_version --ignore=src --ignore=e2e --ignore=.*\\.conf\\.js --ignore=\"(angular-cli|tsconfig)\\.json\" --ignore=webpack.*\\.js --out=packages --all --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/flex-layout": "2.0.0-rc.1",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "angular2-moment": "1.2.0",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "ng2-youtube-player": "0.0.1",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.0",
    "@angular/compiler-cli": "2.4.8",
    "@types/electron": "1.4.32",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "@types/youtube": "0.0.28",
    "codelyzer": "2.0.1",
    "concurrently": "3.3.0",
    "devtron": "1.4.0",
    "electron": "1.4.15",
    "electron-debug": "1.1.0",
    "electron-packager": "8.5.2",
    "electron-prebuilt": "1.4.13",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-electron": "5.1.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.1",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "typescript": "2.2.1"
  }
